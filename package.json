{
  "name": "leapopt-ui",
  "version": "0.1.0",
  "description": "My Portal webapp",
  "main": "index.js",
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": "https://gsd.akamai.com/jira/"
  },
  "publishConfig": {
    "registry": "https://lunabuild.akamai.com/nexus/content/repositories/npm-internal/"
  },
  "scripts": {
    "start": "./node_modules/.bin/gulp",
    "build": "npm run clean && npm run test && ./node_modules/.bin/gulp build --production",
    "test": "./node_modules/.bin/gulp test --production",
    "clean": "./node_modules/.bin/gulp clean",
    "locale": "mkdir -p reports/unit && ./node_modules/.bin/sync-locale",
    "locale-jenkins": "npm run locale -- --xunitreport --reportdir=./reports/unit/",
    "locale-update": "npm run locale -- --update"
  },
  "files": [
    "../src/main/resources/static"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "optional": [
            "spec.protoToAssign"
          ],
          "loose": "es6.classes"
        }
      ],
      "browserify-shim",
      "partialify",
      "brfs"
    ]
  },
  "browserify-shim": {
    "angular": "global:angular"
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.3",
    "angular-aria": "^1.5.3",
    "angular-material": "^1.0.7",
    "angular-messages": "^1.5.3",
    "angular-ui-router": "^0.2.18"
  },
  "devDependencies": {
    "angular-mocks": "1.4.7",
    "babelify": "^6.3.0",
    "brfs": "^1.4.0",
    "browser-sync": "^2.7.7",
    "browserify": "^10.2.4",
    "browserify-istanbul": "^0.2.1",
    "browserify-shim": "^3.8.10",
    "del": "^2.0.2",
    "eslint-config-akamai": "^1.1.0",
    "event-stream": "^3.3.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-if": "^1.2.5",
    "gulp-sass": "^2.1.0",
    "gulp-minify-css": "^1.1.6",
    "gulp-notify": "^2.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.4.1",
    "gulp-util": "^3.0.6",
    "istanbul": "git://github.com/gotwarlost/istanbul.git#source-map",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-browserify": "^4.3.0",
    "karma-coverage": "0.5.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.4",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-spec-reporter": "0.0.20",
    "mkdirp": "^0.5.1",
    "partialify": "^3.1.5",
    "phantomjs": "^1.9.17",
    "portal-fixture-server": "^0.3.1",
    "pretty-hrtime": "^1.0.0",
    "require-dir": "^0.3.0",
    "shelljs": "^0.5.3",
    "sync-locale": "^0.1.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0",
    "yargs": "^3.25.0"
  }
}
